<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LargeTitleText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">38sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="SubheadText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">5sp</item>
    </style>
    <style name="Body" parent="TextAppearance.AppCompat">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">4sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="BrandedSwitch.control" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/blue</item>
        <item name="colorSwitchThumbNormal">@color/white</item>

    </style>

</resources>